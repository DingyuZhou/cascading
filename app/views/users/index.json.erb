[
  <% unless @users.nil? %>
    <% lastIndex = @users.length - 1 %>
    <% @users.each_with_index do |user, index| %>
      <% if index == lastIndex %>
        <%= render partial: "user", locals: {user: user} %>
      <% else %>
        <%= render partial: "user", locals: {user: user} %>,
      <% end %>
    <% end %>
  <% end %>
]
